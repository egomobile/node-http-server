{
    "name": "@egomobile/http-server",
    "version": "0.64.0",
    "description": "Very fast alternative HTTP server to Express, with simple routing and middleware support and which is compatible with Node.js 12 or later.",
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.mjs",
            "require": "./lib/index.cjs",
            "types": "./lib/index.d.ts"
        }
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "build": "del ./lib && rollup -c rollup.config.mjs",
        "docs:generate": "del ./docs && typedoc --out ./docs ./src",
        "lint": "eslint -c .eslintrc.cjs --ext .ts src",
        "lint:fix": "eslint --fix -c .eslintrc.cjs --ext .ts src",
        "preinstall": "node ./preinstall.cjs",
        "sandbox": "NODE_ENV=development nodemon --watch './sandbox/**/*.ts' --watch './sandbox/**/*.cts' --watch './sandbox/**/*.mts' --exec node --experimental-specifier-resolution=node --loader ts-node/esm --inspect=0.0.0.0:9229 ./sandbox/index.ts",
        "test": "jest --no-cache --silent"
    },
    "jest": {
        "preset": "ts-jest/presets/js-with-ts",
        "transform": {},
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "setupFilesAfterEnv": [
            "./src/setupTests.ts"
        ],
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/egomobile/node-http-server.git"
    },
    "keywords": [
        "node",
        "http",
        "http2",
        "http server",
        "backend",
        "router",
        "routing",
        "middleware",
        "server",
        "path matching",
        "express",
        "connect",
        "decorator",
        "typescript",
        "nodejs",
        "microservice",
        "esm",
        "cjs",
        "commonjs",
        "ego"
    ],
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "LGPL-3.0",
    "files": [
        "index.d.ts",
        "CHANGELOG.md",
        "LICENSE",
        "lib",
        "!lib/**/__tests__/**/*",
        "!lib/**/*.map",
        "!lib/**/setupTests.*",
        "preinstall.cjs",
        "README.md"
    ],
    "devDependencies": {
        "@egomobile/tsconfig": "^5.0.0",
        "@rollup/plugin-commonjs": "24.0.1",
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-terser": "0.4.0",
        "@rollup/plugin-typescript": "11.0.0",
        "@types/jest": "29.4.4",
        "@types/node": "18.15.0",
        "@types/supertest": "2.0.12",
        "axios": "1.3.4",
        "babel-jest": "29.5.0",
        "del-cli": "5.0.0",
        "eslint": "8.36.0",
        "eslint-config-ego": "^0.17.1",
        "jest": "29.5.0",
        "nodemon": "2.0.21",
        "rollup": "3.19.1",
        "supertest": "6.3.3",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "tslint": "6.1.3",
        "typedoc": "0.23.26",
        "typescript": "5.0.2"
    }
}