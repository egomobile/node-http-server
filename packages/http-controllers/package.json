{
    "name": "@egomobile/http-controllers",
    "version": "0.0.0",
    "description": "Controller framework for @egomobile/http-server module.",
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.mjs",
            "require": "./lib/index.cjs",
            "types": "./lib/index.d.ts"
        }
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "build": "del ./lib && rollup -c rollup.config.mjs",
        "docs:generate": "del ./docs && typedoc --out ./docs ./src",
        "lint": "eslint -c .eslintrc.cjs --ext .ts src",
        "lint:fix": "eslint --fix -c .eslintrc.cjs --ext .ts src",
        "preinstall": "node ./preinstall.cjs",
        "sandbox": "npm run build && cd ./sandbox && NODE_ENV=development nodemon --watch './**/*.ts' --watch './**/*.cts' --watch './**/*.mts' --exec node --experimental-specifier-resolution=node --loader ts-node/esm --inspect=0.0.0.0:9229 ./index.mts",
        "test": "exit 0",
        "test2": "jest --no-cache --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/egomobile/node-http-server.git"
    },
    "keywords": [
        "node",
        "http",
        "http2",
        "http server",
        "backend",
        "router",
        "routing",
        "middleware",
        "server",
        "path matching",
        "controller",
        "mvc",
        "express",
        "connect",
        "testing",
        "unit test",
        "decorator",
        "typescript",
        "nodejs",
        "microservice",
        "ajv",
        "joi",
        "validation",
        "json",
        "yaml",
        "schema",
        "swagger ui",
        "openapi",
        "documentation",
        "esm",
        "cjs",
        "commonjs",
        "ego"
    ],
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "LGPL-3.0",
    "files": [
        "index.d.ts",
        "CHANGELOG.md",
        "LICENSE",
        "lib",
        "!lib/**/__tests__/**/*",
        "!lib/**/*.map",
        "!lib/**/setupTests.*",
        "preinstall.cjs",
        "README.md"
    ],
    "devDependencies": {
        "@egomobile/http-server": "0.64.0-alpha.360",
        "@egomobile/tsconfig": "^5.0.0",
        "@rollup/plugin-commonjs": "24.0.1",
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-typescript": "11.0.0",
        "@types/js-yaml": "4.0.5",
        "@types/node": "18.15.0",
        "axios": "1.3.4",
        "del-cli": "5.0.0",
        "eslint": "8.36.0",
        "eslint-config-ego": "^0.17.1",
        "nodemon": "2.0.21",
        "rollup": "3.19.1",
        "ts-node": "10.9.1",
        "tslint": "6.1.3",
        "typescript": "5.0.1-rc"
    },
    "peerDependencies": {
        "@egomobile/http-server": ">= 0.64.0-alpha.360"
    },
    "dependencies": {
        "@types/json-schema": "7.0.11",
        "joi": "17.8.4",
        "js-yaml": "4.1.0",
        "json5": "2.2.3",
        "minimatch": "7.4.2"
    }
}
